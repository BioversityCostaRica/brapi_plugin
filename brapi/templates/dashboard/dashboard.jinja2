{% extend_me %}

{% block project_heading_info %}
    <td width="95%">
        <h2 ><strong>{{ activeProject.project_name }}</strong></h2>
    </td>
    <td rowspan="2" class="text-center">
        <a href="https://climmob.net/blog/wiki/main-menu/" target="_blank"><img style="width: 25px; height: 25px " src="{{ request.url_for_static('landing/info.png') }}" alt="info"/> </a>
    </td>
    <td rowspan="2" class="text-center">
    {% if request.h.check_integration("user", "project") == True %}
        <img title="{{ _('This project can be integrated with BreedBase') }}" src="{{ request.url_for_static('BreedBase_pass.png', 'brapri') }}">
    {% else %}
        <img title="{{ _('This project has technologies that are not compatible with BreedBase. Integration is not possible') }}" src="{{ request.url_for_static('BreedBase_fails.png', 'brapri') }}">
    {% endif %}
    </td>
{% endblock project_heading_info %}